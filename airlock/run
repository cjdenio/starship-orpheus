#!/bin/bash

for team in {1..2}
do
    docker run \
    -d \
    --name "airlock-$team" \
    --hostname airlock \
    --restart unless-stopped \
    -p "222$team:22" \
    -e PASSWORD=$(jq -r .[\"$team\"] < passwords.json) \
    -e SPACEDINO_URL=https://starship.clb.li/airlock/start/$team \
    airlock
done
